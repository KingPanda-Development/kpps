<!DOCTYPE html>
<html lang="en" style="background-color: rgba(0, 0, 0, 0); width: 100%; height: 100%">

<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Growtopia Player Support</title>

  <!-- include bootstrap and custom  css -->
  <link media="all" rel="stylesheet" href="/dashboardgt/assets/css/faq-main.css" />
  <!-- include custom  css -->
  <link media="all" rel="stylesheet" href="/dashboardgt/assets/css/shop-custom.css" />
  <link media="all" rel="stylesheet" href="/dashboardgt/assets/css/ingame-custom.css" />
  <style>
    .modal-backdrop {
      background-color: rgba(0, 0, 0, 0.1) !important;
    }

    .modal-backdrop+div {
      overflow: auto;
    }

    .modal-body,
    .content {
      padding: 0;
    }

    #save-login {
      display: flex;
      justify-content: center;
      align-items: center;
      gap: 8px;
      cursor: pointer;
    }

    @keyframes bounce {

      0%,
      100% {
        transform: translateY(1px);
      }

      50% {
        transform: translateY(-8px);
      }
    }

    .anim-bounce {
      animation: bounce 0.5s ease;
    }
  </style>
</head>

<body style="background-color: rgba(0, 0, 0, 0)">
  <button type="button" class="btn btn-primary hidden" data-toggle="modal" id="modalButton" data-target="#modalShow"
    data-backdrop="static" data-keyboard="false"></button>
  <div class="content">
    <section class="common-box">
      <div class="container">
        <div class="row">
          <div class="col-md-12 col-sm-12">
            <div class="row">
              <div class="modal fade product-list-popup" id="modalShow" tabindex="-1" role="dialog" aria-hidden="false">
                <div class="modal-dialog modal-dialog-centered" role="document">
                  <div class="modal-content">
                    <div class="modal-body">
                      <div class="content">
                        <section class="common-box">
                          <div class="container">
                            <div class="section-title center-align">
                              <h2>Log in with your Grow ID</h2>
                            </div>
                            <div class="row div-content-center">
                              <div class="col-md-12 col-sm-12">
                                <form method="POST" action="/player/growid/login/validate" accept-charset="UTF-8"
                                  class="" role="form" required="required" autocomplete="off">
                                  <input name="_token" value="<%= JSON.stringify(data) %>" type="hidden" />
                                  <input id="saveLoginInput" name="saveLogin" type="hidden" value="0" />
                                  <div class="form-group center-align">
                                    <input id="login-name" class="form-control grow-text" required="required"
                                      placeholder="Your Growtopia Name *" name="growId" type="text" />
                                  </div>
                                  <div class="form-group center-align">
                                    <input id="password" class="form-control grow-text" required="required"
                                      placeholder="Your Growtopia Password *" name="password" type="password"
                                      value="" />
                                  </div>
                                  <div class="form-group text-center">
                                    <input class="btn btn-lg btn-primary grow-button" type="submit" value="Log in" />
                                  </div>
                                </form>
                                <div class="form-group text-center forgot-password">
                                  <a href="https://gtfy.me/discord" target="_blank">Need Help? Join GTFY Discord
                                    Server</a>
                                </div>
                                <div class="form-group text-center forgot-password">
                                  <a href="https://gtfy.me/wa" target="_blank">Not Have Discord? Join GTFY Whatsapp
                                    Group</a>
                                </div>
                              </div>
                            </div>
                          </div>
                        </section>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
  </div>
</body>
<!-- JQUERY LIBRARY -->
<script src="/dashboardgt/assets/js/jquery-2.1.4.min.js"></script>
<!-- boostrap javascript -->
<script src="/dashboardgt/assets/js/bootstrap.min.js"></script>
<script>
  $("document").ready(function () {
    document.onkeydown = (e) => {
      if (e.key == 123) {
        e.preventDefault();
      }
      if (e.key == "F12") {
        e.preventDefault();
      }
      if (e.ctrlKey && e.shiftKey && e.key == "I") {
        e.preventDefault();
      }
      if (e.ctrlKey && e.key == "I") {
        e.preventDefault();
      }
      if (e.ctrlKey && e.shiftKey && e.key == "C") {
        e.preventDefault();
      }
      if (e.ctrlKey && e.shiftKey && e.key == "J") {
        e.preventDefault();
      }
      if (e.ctrlKey && e.key == "U") {
        e.preventDefault();
      }
    };
    $("#modalButton").trigger("click");
    $(".close").on("click", function () {
      window.close();
    });
    var observer = new MutationObserver(function (mutations) {
      mutations.forEach(function (mutation) {
        for (var i = 0; i < mutation.addedNodes.length; i++) {
          if (mutation.addedNodes[i].tagName == "DIV") {
            var thediv = mutation.addedNodes[i];
            var sw = window.screen.width;
            if (sw < 667) {
              $(thediv).css({
                transform: "scale(0.75)",
                "transform-origin": "0 0",
                "-webkit-transform": "scale(0.75)",
                "-webkit-transform-origin": "0 0",
                overflow: "auto"
              });
            }
          }
        }
      });
    });
    observer.observe(document.body, { attributes: false, childList: true, characterData: false });
  });
</script>

</html>
